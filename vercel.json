{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ],
  "env": {
    "YOUR_ENV_VARIABLE": "@your-secret-env-variable"
  },
  "functions": {
    "server.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}